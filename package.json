{
	"name": "haoide-vscode",
	"displayName": "haoide-vscode",
	"publisher": "mouseliu",
	"repository": "https://github.com/xjsender/haoide-vscode",
	"description": "haoide-vscode is a vscode extension for force.com development",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.haoide.authorize.new",
				"title": "%extension.haoide.authorize.new.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.authorize.default",
				"title": "%extension.haoide.authorize.default.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.addDefaultProjectToWorkspace",
				"title": "%extension.haoide.addDefaultProjectToWorkspace.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.loginToSFDC",
				"title": "%extension.haoide.loginToSFDC.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.locateThisInBrowser",
				"title": "%extension.haoide.locateThisInBrowser.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.copyLoginUrl",
				"title": "%extension.haoide.copyLoginUrl.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.executeAnonymous",
				"title": "%extension.haoide.executeAnonymous.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.switchProject",
				"title": "%extension.haoide.switchProject.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.toggleMetadataObjects",
				"title": "%extension.haoide.toggleMetadataObjects.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.createProject",
				"title": "%extension.haoide.createProject.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.retrieveThisFromServer",
				"title": "%extension.haoide.retrieveThisFromServer.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.deployThisToServer",
				"title": "%extension.haoide.deployThisToServer.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.reloadSobjectCache",
				"title": "%extension.haoide.reloadSobjectCache.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.formatJson",
				"title": "%extension.haoide.formatJson.title%",
				"category": "Haoide"
			},
			{
				"command": "extension.haoide.convertXml2Json",
				"title": "%extension.haoide.convertXml2Json.title%",
				"category": "Haoide"
			}
		],
		"menus": {
			"editor/context": [
				{
					"group": "z_commands",
					"command": "extension.haoide.retrieveThisFromServer"
				},
				{
					"group": "z_commands",
					"command": "extension.haoide.deployThisToServer"
				},
				{
					"group": "z_commands",
					"command": "extension.haoide.locateThisInBrowser"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.haoide.switchProject",
				"key": "alt+s",
				"mac": "alt+s",
				"win": "alt+s",
				"linux": "alt+s"
			},
			{
				"command": "extension.haoide.executeAnonymous",
				"key": "ctrl+alt+e",
				"mac": "cmd+shift+e",
				"win": "ctrl+alt+e",
				"linux": "ctrl+alt+e"
			},
			{
				"command": "extension.haoide.retrieveThisFromServer",
				"key": "ctrl+alt+r",
				"mac": "ctrl+shift+r",
				"win": "ctrl+alt+r",
				"linux": "ctrl+alt+r"
			},
			{
				"command": "extension.haoide.deployThisToServer",
				"key": "ctrl+alt+s",
				"mac": "cmd+shift+s",
				"win": "ctrl+alt+s",
				"linux": "ctrl+alt+s"
			}
		],
		"languages": [
			{
				"id": "apex",
				"extensions": [
					".cls",
					".trigger"
				],
				"aliases": [
					"Apex",
					"apex"
				],
				"configuration": "./syntaxes/apex-configuration.json"
			},
			{
				"id": "visualforce",
				"aliases": [
					"Visualforce",
					"visualforce"
				],
				"extensions": [
					".page",
					".component"
				],
				"configuration": "./syntaxes/visualforce-configuration.json"
			},
			{
				"id": "html",
				"extensions": [
					".app",
					".cmp",
					".design",
					".evt",
					".intf",
					".auradoc",
					".tokens"
				]
			}
		],
		"grammars": [
			{
				"language": "apex",
				"scopeName": "source.apex",
				"path": "./syntaxes/apex.tmLanguage"
			},
			{
				"language": "visualforce",
				"scopeName": "text.visualforce.markup",
				"path": "./syntaxes/visualforce.json",
				"embeddedLanguages": {
					"source.css": "css",
					"source.js": "javascript"
				}
			},
			{
				"language": "html",
				"scopeName": "text.html.basic",
				"path": "./syntaxes/html-tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "apex",
				"path": "./snippets/trigger.code-snippets"
			},
			{
				"language": "apex",
				"path": "./snippets/class.code-snippets"
			},
			{
				"language": "html",
				"path": "./snippets/lwc.code-snippets"
			},
			{
				"language": "html",
				"path": "./snippets/aura.code-snippets"
			},
			{
				"language": "visualforce",
				"path": "./snippets/visualforce.code-snippets"
			}
		],
		"configuration": {
			"type": "object",
			"title": "haoide",
			"properties": {
				"haoide.enable-debug-log": {
					"type": "boolean",
					"default": true,
					"description": "Indicate whether print debug log"
				},
				"haoide.workspace": {
					"type": "string",
					"default": "",
					"description": "Indicate where to put projects"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node .src/test/runTest.ts"
	},
	"devDependencies": {
		"@salesforce/apex-tmlanguage": "^1.1.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.34.0",
		"@types/express": "^4.16.3",
		"@types/request-promise": "^4.1.44",
		"@types/lodash": "^4.14.136",
		"@types/adm-zip": "^0.4.32",
		"@types/yazl": "^2.4.1",
		"@types/shelljs": "^0.8.5",
		"mocha": "^6.2.0",
		"request": "*",
		"request-promise": "^4.2.4",
		"moment": "^2.24.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"express": "^4.16.4",
		"open": "^6.4.0",
		"querystring": "v0.2.0",
		"lodash": "^4.17.15",
		"fast-xml-parser": "^3.12.19",
		"adm-zip": "^0.4.13",
		"yazl": "^2.5.1",
		"shelljs": "^0.8.1",
		"del": "^4.1.1",
		"event-stream": "^4.0.1",
		"gulp": "^4.0.2",
		"gulp-filter": "^5.1.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"run-sequence": "^2.2.1",
		"vsce": "^1.61.0",
		"vscode-nls-dev": "^3.2.6"
	},
	"dependencies": {
		"@salesforce/apex-tmlanguage": "^1.1.0",
		"@types/adm-zip": "^0.4.32",
		"@types/archiver": "^3.0.0",
		"@types/express": "^4.16.3",
		"@types/lodash": "^4.14.136",
		"@types/node": "^10.12.21",
		"@types/request-promise": "^4.1.44",
		"@types/shelljs": "^0.8.5",
		"@types/yazl": "^2.4.1",
		"adm-zip": "^0.4.13",
		"archiver": "^3.1.1",
		"express": "^4.16.4",
		"fast-xml-parser": "^3.12.19",
		"gulp": "^4.0.2",
		"inherits": "^2.0.4",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"npm-update": "^2.1.0",
		"open": "^6.4.0",
		"querystring": "v0.2.0",
		"request": "*",
		"request-promise": "^4.2.4",
		"rxjs": "^6.5.2",
		"shelljs": "^0.8.1",
		"typescript": "^3.3.1",
		"vscode-nls": "^3.2.5",
		"yazl": "^2.5.1"
	}
}
