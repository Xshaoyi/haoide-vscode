			{
				"command": "extension.haoide.setSyntaxToApex",
				"title": "%extension.haoide.setSyntaxToApex.title%",
				"icon": {
					"light": "./resources/media/svgs/language.svg",
					"dark": "./resources/media/svgs/language.svg"
				},
				"category": "Haoide"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "extension.haoide.authorize.new"
				},
				{
					"command": "extension.haoide.authorize.default",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.addDefaultProjectToWorkspace",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.loginToSFDC",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.locateThisInBrowser",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.copyLoginUrl",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.executeAnonymous",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.executeQuery",
					"when": "haoide.hasOpenProject && editorHasSelection"
				},
				{
					"command": "extension.haoide.executeToolingQuery",
					"when": "haoide.hasOpenProject && editorHasSelection"
				},
				{
					"command": "extension.haoide.switchProject",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.toggleMetadataObjects",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.describeMetadata",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createNewProject",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.updateProject",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.updateUserLanguage",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.retrieveThisFromServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.retrieveOpenFilesFromServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.destructThisFromServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.destructOpenFilesFromServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.deployThisToServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.deployOpenFilesToServer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.startRestExplorer",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.reloadSobjectCache",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.reloadSymbolTable",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.formatJson",
					"when": "editorHasSelection"
				},
				{
					"command": "extension.haoide.convertXml2Json",
					"when": "editorHasSelection"
				},
				{
					"command": "extension.haoide.convertJson2Apex",
					"when": "editorHasSelection"
				},
				{
					"command": "extension.haoide.convertJson2Typescript",
					"when": "editorHasSelection"
				},
				{
					"command": "extension.haoide.convert15IdTo18Id"
				},
				{
					"command": "extension.haoide.createLightningWebComponent",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createAuraComponent",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createApexClass",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createApexTrigger",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createVisualforcePage",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.createVisualforceComponent",
					"when": "haoide.hasOpenProject"
				},
				{
					"command": "extension.haoide.startRestExplorer",
					"when": "haoide.hasOpenProject"
				}
			],
			"editor/context": [
				{
					"group": "z_commands",
					"command": "extension.haoide.retrieveThisFromServer"
				},
				{
					"group": "z_commands",
					"command": "extension.haoide.deployThisToServer"
				},
				{
					"group": "z_commands",
					"command": "extension.haoide.destructThisFromServer"
				},
				{
					"group": "z_commands",
					"command": "extension.haoide.locateThisInBrowser"
				}
			],
			"editor/title": [{
				"command": "extension.haoide.setSyntaxToApex",
				"group": "navigation"
			}]
		},
		"keybindings": [
			{
				"command": "extension.haoide.switchProject",
				"key": "alt+s",
				"mac": "alt+s",
				"win": "alt+s",
				"linux": "alt+s"
			},
			{
				"command": "extension.haoide.executeAnonymous",
				"key": "ctrl+alt+e",
				"mac": "ctrl+alt+e",
				"win": "ctrl+alt+e",
				"linux": "ctrl+alt+e"
			},
			{
				"command": "extension.haoide.updateUserLanguage",
				"key": "alt+u",
				"mac": "alt+u",
				"win": "alt+u",
				"linux": "alt+u"
			},
			{
				"command": "extension.haoide.retrieveThisFromServer",
				"key": "ctrl+alt+r",
				"mac": "ctrl+shift+r",
				"win": "ctrl+alt+r",
				"linux": "ctrl+alt+r"
			},
			{
				"command": "extension.haoide.deployThisToServer",
				"key": "ctrl+alt+s",
				"mac": "cmd+shift+s",
				"win": "ctrl+alt+s",
				"linux": "ctrl+alt+s"
			}
		],
		"languages": [
			{
				"id": "apex",
				"extensions": [
					".cls",
					".trigger"
				],
				"aliases": [
					"Apex",
					"apex"
				],
				"configuration": "./resources/syntaxes/apex-configuration.json"
			},
			{
				"id": "visualforce",
				"aliases": [
					"Visualforce",
					"visualforce"
				],
				"extensions": [
					".page",
					".component"
				],
				"configuration": "./resources/syntaxes/visualforce-configuration.json"
			},
			{
				"id": "html",
				"extensions": [
					".app",
					".cmp",
					".design",
					".evt",
					".intf",
					".auradoc",
					".tokens"
				]
			}
		],
		"grammars": [
			{
				"language": "apex",
				"scopeName": "source.apex",
				"path": "./resources/syntaxes/apex.tmLanguage"
			},
			{
				"language": "visualforce",
				"scopeName": "text.visualforce.markup",
				"path": "./resources/syntaxes/visualforce.json",
				"embeddedLanguages": {
					"source.css": "css",
					"source.js": "javascript"
				}
			},
			{
				"language": "html",
				"scopeName": "text.html.basic",
				"path": "./resources/syntaxes/html-tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "apex",
				"path": "./resources/snippets/trigger.code-snippets"
			},
			{
				"language": "apex",
				"path": "./resources/snippets/class.code-snippets"
			},
			{
				"language": "html",
				"path": "./resources/snippets/lwc.code-snippets"
			},
			{
				"language": "html",
				"path": "./resources/snippets/aura.code-snippets"
			},
			{
				"language": "visualforce",
				"path": "./resources/snippets/visualforce.code-snippets"
			}
		],
		"configuration": {
			"type": "object",
			"title": "haoide",
			"properties": {
				"haoide.enableDebugLog": {
					"type": "boolean",
					"default": false,
					"description": "Indicate whether print debug log"
				},
				"haoide.workspace": {
					"type": "string",
					"default": "",
					"description": "Indicate where to put projects"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -b -w",
		"compile": "tsc -b",
		"test": "node ./out/test/runTests.js"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^12.0.1",
		"@types/vscode": "^1.35.0",
