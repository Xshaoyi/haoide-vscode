		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"mocha": "^6.2.0",
		"glob": "^7.1.3",
		"run-sequence": "^2.2.1",
		"tslint": "^5.19.0",
		"typescript": "^3.5.1",
		"vsce": "^1.61.0",
		"vscode-nls-dev": "^3.2.6",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@salesforce/apex-tmlanguage": "^1.1.0",
		"@types/adm-zip": "^0.4.32",
		"@types/express": "^4.16.3",
		"@types/lodash": "^4.14.136",
		"@types/request-promise": "^4.1.44",
		"@types/shelljs": "^0.8.5",
		"@types/yazl": "^2.4.1",
		"promise-limit": "^2.7.0",
		"cometd": "^3.1.10",
		"adm-zip": "^0.4.13",
		"express": "^4.16.4",
		"fast-xml-parser": "^3.12.19",
		"inherits": "^2.0.4",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"open": "^6.4.0",
		"querystring": "v0.2.0",
		"request": "*",
		"request-promise": "^4.2.4",
		"shelljs": "^0.8.1",
		"vscode-nls": "^3.2.5",
		"yazl": "^2.5.1"
	}
}
